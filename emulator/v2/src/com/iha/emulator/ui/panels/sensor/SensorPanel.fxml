<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<VBox fx:id="view" maxWidth="242.0" prefHeight="249.0" styleClass="sensorPanel" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iha.emulator.ui.panels.sensor.SensorPanelView">
   <children>
      <Label fx:id="nameLbl" prefHeight="59.0" styleClass="sensorPanelHeader" text="Multisensor" VBox.vgrow="NEVER" />
      <HBox alignment="CENTER" prefHeight="25.0" prefWidth="205.0" VBox.vgrow="NEVER">
         <children>
            <Label fx:id="batteryLbl" prefHeight="16.0" prefWidth="67.0" text="100%" HBox.hgrow="NEVER">
               <styleClass>
                  <String fx:value="sensorBattery" />
                  <String fx:value="plainTextColor" />
                  <String fx:value="withPlainContextMenu" />
                  <String fx:value="withSlider" />
               </styleClass>
            </Label>
            <Label fx:id="signalLbl" prefHeight="16.0" prefWidth="67.0" text="100%" HBox.hgrow="NEVER">
               <styleClass>
                  <String fx:value="sensorSignal" />
                  <String fx:value="plainTextColor" />
               </styleClass>
            </Label>
            <Label fx:id="refreshTimeLbl" prefHeight="16.0" prefWidth="80.0" text="10 min" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="sensorRefresh" />
                  <String fx:value="plainTextColor" />
                  <String fx:value="boldText" />
               </styleClass>
            </Label>
         </children>
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
      </HBox>
      <Separator prefWidth="200.0" VBox.vgrow="NEVER">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Separator>
      <GridPane prefHeight="36.0" prefWidth="205.0" styleClass="sensorText">
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="93.0" styleClass="sensorText" text="Status:" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
            <Label alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="83.0" styleClass="sensorText" text="Id:" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS" />
            <Label fx:id="statusLbl" prefHeight="14.0" prefWidth="125.0" styleClass="sensorText" text="Active" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <Label fx:id="idLbl" prefHeight="14.0" prefWidth="125.0" styleClass="sensorId" text="0.100.0.1" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="99.0" minWidth="10.0" prefWidth="85.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="132.0" minWidth="10.0" prefWidth="112.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <Separator prefWidth="200.0">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Separator>
      <TableView fx:id="valueTable" prefHeight="93.0" prefWidth="205.0" styleClass="valuesTable" VBox.vgrow="NEVER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn editable="false" sortable="false" />
            <TableColumn editable="false" sortable="false" />
         </columns>
         <padding>
            <Insets left="3.0" right="3.0" />
         </padding>
      </TableView>
      <Separator prefWidth="200.0">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Separator>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="205.0" spacing="20.0" VBox.vgrow="NEVER">
         <children>
            <Button fx:id="settingsBtn" mnemonicParsing="false" onAction="#handleSettings" text="Settings" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="sensorControlBtn" />
                  <String fx:value="sensorSettingsBtn" />
               </styleClass>
            </Button>
            <Button fx:id="connectionBtn" mnemonicParsing="false" onAction="#handleConnection" text="Connect" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="sensorControlBtn" />
                  <String fx:value="sensorEnableBtn" />
               </styleClass>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
