<devices>
	<device id="0">
		<name>Pressure Sensor</name>
		<vendor>BeeeOn</vendor>

		<match>
			<exact name="Air Pressure Sensor" vendor="BeeeOn" />
		</match>

		<modules>
			<sensor id="0" type="15">
				<name>Pressure</name>
			</sensor>
		</modules>
	</device>

	<device id="10">
		<name>Remote Switch</name>
		<vendor>Belkin WeMo</vendor>

		<match>
			<exact name="Switch F7C027fr" vendor="Belkin WeMo" />
		</match>

		<modules>
			<control id="0" type="13">
				<name>On/Off</name>
			</control>
		</modules>
	</device>

	<device id="11">
		<name>Bulb</name>
		<vendor>Belkin WeMo</vendor>

		<match>
			<exact name="Led Light Bulb" vendor="Belkin WeMo" />
		</match>

		<modules>
			<control id="0" type="13">
				<name>On/Off</name>
			</control>
			<control id="1" type="4">
				<name>Brightness</name>
			</control>
		</modules>
	</device>
</devices>
